{
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "TUID Helper"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/content.js"]
    }
  ],
  "description": "Helper extension to generate and disect TUID.",
  "name": "TUID Helper",
  "version": "0.7",
  "minimum_chrome_version": "9",
  "manifest_version": 2,
  "permissions":["tabs", "storage"]
}